<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="dependency_injection.constructor_injecttion.ci_with_primitive_datatype.Student">
        <constructor-arg name="sid" value="1" type="int"/>
        <constructor-arg name="name" value="Luong Ba Hoang"/>
        <constructor-arg name="address" value="Thanh Hoa"/>
    </bean>
</beans>
